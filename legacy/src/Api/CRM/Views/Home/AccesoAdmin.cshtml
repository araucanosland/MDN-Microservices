<body>
    <div id="container" class="effect aside-float aside-bright mainnav-lg">
        <div class="panel">
            <div class="panel-heading">
                <h3 class="panel-title">Acceso Como Ejecutivo</h3>
            </div>
            <div class="panel-body">

                <!-- Inline Form  -->
                <!--===================================================-->
                <form class="form-inline">
                    <div class="form-group">
                        <label for="demo-inline-inputmail" class="sr-only">Rut Ejecutivo</label>
                        <input type="text" placeholder="Rut Ejecutivo" name="RE" class="form-control" required>
                    </div>
                    <button class="btn btn-primary" type="submit">Acceder</button>
                </form>
                

                <!--Grilla-->
                <table id="demo-foo-filtering" class="table table-bordered table-hover toggle-circle" data-page-size="50">
                    <thead>
                        <tr>
                            <th data-toggle="true">Rut</th>
                            <th>Nombre</th>
                            <th data-hide="phone, tablet">Codigo Sucursal</th> 
                            <th data-hide="phone, tablet">Sucursal</th> 
                            <th data-hide="phone, tablet">Cargo</th>                            
                        </tr>
                    </thead>
                    <div class="pad-btm">
                        <div class="row">
                            <div class="col-sm-3 text-xs-center">
                                <label class="control-label">Búsqueda</label>
                                <input id="demo-foo-search" type="text" placeholder="Ej. 11.111.111-1" class="form-control" autocomplete="off">
                            </div>

                        </div>
                    </div>
                    <tbody id="bdy_datos"></tbody>
                    <tfoot>
                        <tr>
                            <td colspan="10">
                                <div class="text-right">
                                    <ul class="pagination"></ul>
                                </div>
                            </td>
                        </tr>
                    </tfoot>
                </table>
            
            
            </div>
        </div>
